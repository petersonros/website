{
  "name": "website",
  "homepage": "https://petersonros.github.io/website/",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:hg": "cross-env NEXT_PUBLIC_SITE_URL=https://petersonros.com next build",
    "build:gh": "cross-env GITHUB_PAGES=true NEXT_PUBLIC_SITE_URL=https://petersonros.github.io/website next build",
    "preview:out": "npx serve -l 3000 out",
    "deploy:ftp": "node scripts/ftp-deploy.mjs",
    "deploy:hg": "npm run build:hg && npm run deploy:ftp",
    "start": "next start",
    "lint": "next lint",
    "build": "next build",
    "predeploy": "touch out/.nojekyll",
    "deploy": "gh-pages -d out --dotfiles",
    "upload": "node ftp-deploy.js"
  },
  "dependencies": {
    "@react-three/drei": "^10.6.0",
    "@react-three/fiber": "^9.2.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "swiper": "^11.2.10",
    "three": "^0.178.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ftp-deploy": "^2.4.7",
    "tree": "^0.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.178.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "ftp-deploy": "^2.4.7",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "cross-env": "^7.0.3"
  }
}
